{
  "type": "module",
  "name": "seeded-color",
  "version": "1.0.1",
  "description": "generate a psuedorandom color from a seed",
  "main": "pkg/index.js",
  "types": "src/index.ts",
  "esnext": "pkg/index.js",
  "files": [
    "pkg/*",
    "src/index.ts"
  ],
  "repository": "https://github.com/Brecert/seeded-numbers",
  "author": "bree",
  "license": "MIT",
  "scripts": {
    "build": "tsc && denoify",
    "test": "denoify && deno test ./deno",
    "prepublishOnly": "yarn test && yarn build"
  },
  "devDependencies": {
    "denoify": "^0.6.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "seeded-numbers": "^1.0.2"
  },
  "denoify": {
    "ports": {
      "seeded-numbers": "https://raw.githubusercontent.com/Brecert/seeded-numbers/main/mod.ts"
    },
    "includes": [
      "README.md",
      "LICENSE"
    ]
  }
}
